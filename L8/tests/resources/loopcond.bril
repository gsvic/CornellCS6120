# ARGS: dom
@main {
.entry:
  x: int = const 0;
  i: int = const 0;
  j: int = const 0;
  one: int = const 1;
  z: int = const 1;

.loop:
  inv: int = mul one z;
  uses_inv: int = add inv z;
  max: int = const 1000000;
  cond: bool = lt i max;
  br cond .body .exit;

.body:
  x: int = add x one;
  jmp .endif;

.endif:
  factor: int = const 2;
  x: int = mul x factor;

  i: int = add i one;
  jmp .loop;

.exit:
  print i;
}
